}


@section headerScripts {
    <link rel="stylesheet"
      href="~/vendor/swiper-bundle.min.css" />
    <link rel="stylesheet" href="~/vendor/animate.min.css" />
    <style>
        .swiper-button-next{
            right: 40px;
            background: url(./assets/svg/chevron-right.svg), rgba(0,0,0,0.3);
        }
        .swiper-button-prev{
            left: 40px;
            background: url(./assets/svg/chevron-left.svg), rgba(0,0,0,0.3);
        }

        .swiper-button-next,
        .swiper-button-prev{
            top: 30%;
            width: 50px;
            height: 50px;
            border: 3px solid black;
            border-radius: 50%;
            background-repeat: no-repeat;
            background-size: 100% auto;
            background-position: center;
            filter: invert(100%);
            opacity: 0.5;
            transition: opacity 300ms ease;
        }

            .swiper-button-next:hover,
            .swiper-button-prev:hover{
                opacity: 1;
            }

            .swiper-button-next:after,
            .swiper-button-prev:after{
                display: none;
            }
    </style>
}

